Sub mapDraw(screen as Ubyte)
    Dim index As Uinteger
    Dim y, x As Ubyte
    
    x = 0
    y = 0
    
    For index=0 To SCREEN_LENGTH - 1
        drawTile(map(screen, y, x), x, y)
        
        x = x + 1
        If x = SCREEN_WIDTH - 1 Then
            x = 0
            y = y + 1
        End If
    Next index
End Sub